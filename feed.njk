---
permalink: /feed.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>PHDEV Notebooks</title>
    <link>https://notebooks.phdev.uk</link>
    <description>分享程式開發筆記與專案連結</description>
    <language>zh-TW</language>
    <lastBuildDate>{{ collections.recentPosts[0].data.date | isoDate }}</lastBuildDate>
    <atom:link href="https://notebooks.phdev.uk/feed.xml" rel="self" type="application/rss+xml"/>
    
    {%- for post in collections.recentPosts -%}
    <item>
      <title>{{ post.data.title | escape }}</title>
      <link>{{ post.data.url }}</link>
      <description>{{ (post.data.description or post.data.title) | stripHtml | truncate | escape }}</description>
      <pubDate>{{ post.data.date | isoDate }}</pubDate>
      <guid isPermaLink="false">{{ post.data.url }}-{{ post.data.date | date('YYYY-MM-DD') }}</guid>
      {%- if post.data.tags -%}
        {%- for tag in post.data.tags -%}
        <category>{{ tag }}</category>
        {%- endfor -%}
      {%- endif -%}
    </item>
    {%- endfor -%}
  </channel>
</rss>